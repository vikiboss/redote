{
  "name": "redote",
  "version": "0.0.0-alpha.0",
  "description": "Modern TypeScript framework for Xiaohongshu (RedBook) direct message bot",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "build": "bun build src/index.ts --outdir dist",
    "start": "bun run src/index.ts",
    "test": "bun test",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": ["xiaohongshu", "redbook", "bot", "typescript", "websocket"],
  "author": "Viki <hi@viki.moe> (https://github.com/vikiboss)",
  "license": "MIT",
  "packageManager": "pnpm@10.15.0",
  "dependencies": {
    "ws": "^8.16.0",
    "zod": "^3.22.4",
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "@types/ws": "^8.5.10",
    "@types/node": "^24.3.0",
    "typescript": "^5.3.3"
  }
}
